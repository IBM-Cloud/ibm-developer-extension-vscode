    "cli": {
      "command": "ibmcloud",
      "version": "2.7.0",
      "url": "https://cloud.ibm.com/docs/cli"
    },
    "plugins": [
      {
        "displayName": "dev",
        "command": "dev",
        "version": "2.10.0",
        "url": "https://cloud.ibm.com/docs/cli?topic=cli-idt-cli"
