{
  "name": "ibm-developer",
  "displayName": "IBM Developer Tools",
  "description": "Extension for VS Code editor to enable IBMCloud developer `ibmcloud dev` CLI capabilities from within the editing environment.",
  "version": "0.0.15",
  "publisher": "IBM",
  "license": "Apache 2.0 (see LICENSE.txt)",
  "bugs": {
    "url": "https://github.com/IBM-Cloud/ibm-developer-extension-vscode/issues"
  },
  "homepage": "https://github.com/IBM-Cloud/ibm-developer-extension-vscode/blob/master/README.md",
