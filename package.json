{
  "name": "ibm-developer",
  "displayName": "IBM Cloud CLI",
  "description": "Extension for VS Code editor to enable IBM Cloud CLI capabilities from within the editing environment.",
  "version": "1.0.1",
  "publisher": "IBM",
  "license": "Apache 2.0 (see LICENSE.txt)",
  "bugs": {
    "url": "https://github.com/IBM-Cloud/ibm-developer-extension-vscode/issues"
  },
  "homepage": "https://github.com/IBM-Cloud/ibm-developer-extension-vscode/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM-Cloud/ibm-developer-extension-vscode"
  },
  "engineStrict": true,
  "engines": {
    "vscode": "^1.64.0",
    "node": ">=14.0.1 <14.6.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "IBM",
    "Development",
    "Tools",
    "CLI",
    "Bluemix",
    "Cloud"
  ],
  "icon": "assets/icon.png",
  "activationEvents": [
    "onCommand:extension.ibmcloud.api",
    "onCommand:extension.ibmcloud.regions",
    "onCommand:extension.ibmcloud.target",
    "onCommand:extension.ibmcloud.login",
    "onCommand:extension.ibmcloud.login.sso",
    "onCommand:extension.ibmcloud.logout",
    "onCommand:extension.ibmcloud.cli-update",
    "onCommand:extension.ibmcloud.cli-install",
    "onCommand:extension.ibmcloud.ks.cluster.get",
    "onCommand:extension.ibmcloud.ks.cluster.rm",
    "onCommand:extension.ibmcloud.ks.clusters",
    "onCommand:extension.ibmcloud.ks.init",
    "onCommand:extension.ibmcloud.ks.worker.add",
    "onCommand:extension.ibmcloud.ks.worker.get",
    "onCommand:extension.ibmcloud.ks.worker.reboot",
    "onCommand:extension.ibmcloud.ks.worker.reload",
    "onCommand:extension.ibmcloud.ks.worker.rm",
    "onCommand:extension.ibmcloud.ks.workers",
    "onCommand:extension.ibmcloud.account.list",
    "onCommand:extension.ibmcloud.account.show",
    "onCommand:extension.ibmcloud.account.users",
    "onCommand:extension.ibmcloud.resource.service-instances",
    "onCommand:extension.ibmcloud.plugin.install",
    "onCommand:extension.ibmcloud.plugin.uninstall",
    "onCommand:extension.ibmcloud.plugin.update",
    "onCommand:extension.ibmcloud.iam.oauth-tokens",
    "onCommand:extension.ibmcloud.iam.service-id",
    "onCommand:extension.ibmcloud.iam.service-ids",
    "onCommand:extension.ibmcloud.resource.service-binding.get",
    "onCommand:extension.ibmcloud.resource.service-binding.list",
    "onCommand:extension.ibmcloud.resource.service-alias.get",
    "onCommand:extension.ibmcloud.resource.service-alias.list"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.ibmcloud.api",
        "title": "ibmcloud api"
      },
      {
        "command": "extension.ibmcloud.regions",
        "title": "ibmcloud regions"
      },
      {
        "command": "extension.ibmcloud.target",
        "title": "ibmcloud target"
      },
      {
        "command": "extension.ibmcloud.login",
        "title": "ibmcloud login"
      },
      {
        "command": "extension.ibmcloud.login.sso",
        "title": "ibmcloud login --sso"
      },
      {
        "command": "extension.ibmcloud.logout",
        "title": "ibmcloud logout"
      },
      {
        "command": "extension.ibmcloud.cli-update",
        "title": "ibmcloud plugin update --all -r 'IBM Cloud' (update cli extensions)"
      },
      {
        "command": "extension.ibmcloud.cli-install",
        "title": "ibmcloud plugin install --all -r 'IBM Cloud' -f (install cli extensions)"
      },
      {
        "command": "extension.ibmcloud.ks.cluster.get",
        "title": "ibmcloud ks cluster get"
      },
      {
        "command": "extension.ibmcloud.ks.cluster.rm",
        "title": "ibmcloud ks cluster rm"
      },
      {
        "command": "extension.ibmcloud.ks.clusters",
        "title": "ibmcloud ks clusters"
      },
      {
        "command": "extension.ibmcloud.ks.init",
        "title": "ibmcloud ks init"
      },
      {
        "command": "extension.ibmcloud.ks.worker.add",
        "title": "ibmcloud ks worker add"
      },
      {
        "command": "extension.ibmcloud.ks.worker.get",
        "title": "ibmcloud ks worker get"
      },
      {
        "command": "extension.ibmcloud.ks.worker.reboot",
        "title": "ibmcloud ks worker reboot"
      },
      {
        "command": "extension.ibmcloud.ks.worker.reload",
        "title": "ibmcloud ks worker reload"
      },
      {
        "command": "extension.ibmcloud.ks.worker.rm",
        "title": "ibmcloud ks worker rm"
      },
      {
        "command": "extension.ibmcloud.ks.workers",
        "title": "ibmcloud ks workers"
      },
      {
        "command": "extension.ibmcloud.account.list",
        "title": "ibmcloud account list"
      },
      {
        "command": "extension.ibmcloud.account.show",
        "title": "ibmcloud account show"
      },
      {
        "command": "extension.ibmcloud.account.users",
        "title": "ibmcloud account users"
      },
      {
        "command": "extension.ibmcloud.resource.service-instances",
        "title": "ibmcloud resource service-instances"
      },
      {
        "command": "extension.ibmcloud.plugin.install",
        "title": "ibmcloud plugin install"
      },
      {
        "command": "extension.ibmcloud.plugin.uninstall",
        "title": "ibmcloud plugin uninstall"
      },
      {
        "command": "extension.ibmcloud.plugin.update",
        "title": "ibmcloud plugin update"
      },
      {
        "command": "extension.ibmcloud.iam.oauth-tokens",
        "title": "ibmcloud iam oauth-tokens"
      },
      {
        "command": "extension.ibmcloud.iam.service-id",
        "title": "ibmcloud iam service-id"
      },
      {
        "command": "extension.ibmcloud.iam.service-ids",
        "title": "ibmcloud iam service-ids"
      },
      {
        "command": "extension.ibmcloud.resource.service-binding.get",
        "title": "ibmcloud resource service-binding"
      },
      {
        "command": "extension.ibmcloud.resource.service-binding.list",
        "title": "ibmcloud resource service-bindings"
      },
      {
        "command": "extension.ibmcloud.resource.service-alias.get",
        "title": "ibmcloud resource service-alias"
      },
      {
        "command": "extension.ibmcloud.resource.service-alias.list",
        "title": "ibmcloud resource service-aliases"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build:dev": "npm run clean && webpack --mode development",
    "build": "npm run clean && webpack --mode production --devtool hidden-source-map",
    "clean": "rm -rf out",
    "compile": "tsc -p ./",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "detect_secrets": "./scripts/detect_secrets",
    "vscode:publish": "./scripts/publish"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.0.1",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.11",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^4.3.6",
    "eslint": "^8.13.0",
    "mocha": "^10.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^13.0.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "js-yaml": "^3.13.1",
    "ps-tree": "^1.1.0",
    "semver": "^7.5.2",
    "stream-browserify": "^3.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "ibm": {
    "cli": {
      "command": "ibmcloud",
      "version": "2.18.0",
      "url": "https://cloud.ibm.com/docs/cli"
    },
    "plugins": [
      {
        "displayName": "container-service[kubernetes-service]",
        "command": "ks",
        "version": "1.0.353",
        "url": "https://cloud.ibm.com/docs/containers?topic=containers-kubernetes-service-cli"
      }
    ]
  }
}
